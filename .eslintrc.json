{
  "extends": ["eslint-config-prettier", "next/core-web-vitals"],
  "rules": {
    "no-unused-vars": "warn",
    "no-multiple-empty-lines": "warn",
    "no-tabs": "warn",
    "indent": ["warn", 2],
    "quotes": ["warn", "single"],
    "jsx-quotes": ["warn", "prefer-single"],
    "semi": ["error", "never"],
    "eqeqeq": "error"
  }
}
